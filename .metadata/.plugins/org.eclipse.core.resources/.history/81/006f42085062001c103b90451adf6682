
public class swapnum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int a=40;
		int b=20;
		
		a=a^b; // 5+10=15 XOR 
		b=a^b; // 15-10=5
		a=a^b; //15-5=10
		
//		System.out.println(a);
//		System.out.println(b);
//		
//		b = a + b - (a = b);
		
		
		System.out.println(a);
		System.out.println(b);
	}

}
